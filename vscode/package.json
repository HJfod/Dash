{
    "name": "dash-lang-support",
    "displayName": "Dash Language Support",
    "description": "Support for the Dash language for Geometry Dash mods",
    "author": {
        "name": "HJfod",
        "url": "https://github.com/hjfod"
    },
    "repository": {
        "url": "https://github.com/hjfod/dash"
    },
    "publisher": "HJfod",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.77.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dash",
                "aliases": ["Dash"],
                "extensions": [".dash"],
                "configuration": "./language-configuration.json",
                "icon": {
                    "dark": "./logo.png",
                    "light": "./logo.png"
                }
            },
            {
                "id": "dash-grammar",
                "filenames": ["DashGrammar"]
            }
        ],
        "grammars": [
            {
                "language": "dash",
                "scopeName": "source.dash",
                "path": "./syntaxes/dash.tmLanguage.json"
            },
            {
                "language": "dash-grammar",
                "scopeName": "source.dash-grammar",
                "path": "./syntaxes/dash-grammar.tmLanguage.json"
            }
        ]
    }
}