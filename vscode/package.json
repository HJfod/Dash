{
    "name": "gdml-lang-support",
    "displayName": "GDML Language Support",
    "description": "Support for the GDML language for Geometry Dash mods",
    "author": {
        "name": "HJfod",
        "url": "https://github.com/hjfod"
    },
    "repository": {
        "url": "https://github.com/hjfod/gdml"
    },
    "publisher": "HJfod",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.77.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gdml",
            "aliases": ["GDML", "GDML"],
            "extensions": [".gdml"],
            "configuration": "./language-configuration.json",
            "icon": {
                "dark": "./logo.png",
                "light": "./logo.png"
            }
        }],
        "grammars": [{
            "language": "gdml",
            "scopeName": "source.gdml",
            "path": "./syntaxes/gdml.tmLanguage.json"
        }]
    }
}