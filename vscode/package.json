{
    "name": "geo-lang-support",
    "displayName": "Geo Language Support",
    "description": "Support for the Geo language for Geometry Dash mods",
    "author": {
        "name": "HJfod",
        "url": "https://github.com/hjfod"
    },
    "repository": {
        "url": "https://github.com/hjfod/geo"
    },
    "publisher": "HJfod",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.77.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "geo",
            "aliases": ["Geo"],
            "extensions": [".geo"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "geo",
            "scopeName": "source.geo",
            "path": "./syntaxes/geo.tmLanguage.json"
        }]
    }
}