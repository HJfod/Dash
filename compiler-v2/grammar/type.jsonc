{
    "type-expr": {
        "grammar": [
            { "if": { "peek": "ident" }, "then": [{ "return": "#type-ident" }] },
            { "expected": "type expression" }
        ]
    },
    "type-ident": {
        "members": ["ident"],
        "grammar": [
            { "match": "#ident-path", "into": "ident" }
        ]
    }
}
